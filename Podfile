  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for KVOTestModel
  pod 'FMDB', '~> 2.7.5'
  pod 'LookinServer', :configurations => ['Debug']
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'SDWebImage'
  pod 'FLAnimatedImage'


end
